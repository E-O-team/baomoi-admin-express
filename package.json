{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "scripts": {
    "baomoi-admin-install": "cd baomoi-admin && npm install",
    "start": "babel-node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "nodemon --exec babel-node server.js",
    "baomoi-admin": "npm start --prefix baomoi-admin",
    "dev": "concurrently \"npm run server\" \"npm run baomoi-admin\""
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "expo-server-sdk": "^3.2.0",
    "express": "^4.16.2"
  }
}
